<Page x:Class="KeMBody.UI.Pages.SummaryPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:viewmodels="clr-namespace:KeMBody.ViewModels;assembly=KeMBody.ViewModels" d:DataContext="{d:DesignInstance Type=viewmodels:SummaryViewModel}"
      mc:Ignorable="d" Foreground="Transparent">

    <Grid Margin="0,0,-81,0">
        <TextBlock
                Grid.Column="0"
                Grid.Row="0"
                Padding="20"
                FontFamily="Futura"
                FontSize="20"
                FontWeight="Medium"
                Foreground="White"
                Text="Dziś nie zostały dodane pomiary" Height="50"/>
        <Button Height="50" Width="150" Style="{StaticResource Button}"
                    Content="Dodaj pomiary" Margin="228,205,246,145"/>
    </Grid>
</Page>
